    <ImageView
        android:id="@+id/imageView"
        android:layout_width="402dp"
        android:layout_height="195dp"
        android:scaleType="centerCrop"
        android:src="@drawable/dash_header_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/dash_header_1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="ABOUT US"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="152dp"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:elevation="3dp"
        android:foregroundGravity="center"
        app:cardCornerRadius="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_adit"
                android:layout_width="match_parent"
                android:layout_height="152dp"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:clickable="true"
                android:scaleType="centerCrop"
                android:src="@drawable/farmer"
                tools:srcCompat="@drawable/farmer" />

            <TextView
                android:id="@+id/tv_adit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_adit"
                android:paddingLeft="9dp"
                android:paddingRight="9dp"
                android:paddingTop="9dp"
                android:textColor="@color/accent"
                android:text="Adit"
                android:textAlignment="center"
                android:textSize="18sp" />

        </RelativeLayout>



    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view2"
        android:layout_width="152dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:elevation="3dp"
        android:foregroundGravity="center"
        app:cardCornerRadius="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_dekin"
                android:layout_width="match_parent"
                android:layout_height="152dp"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:clickable="true"
                android:scaleType="centerCrop"
                android:src="@drawable/farmer"
                tools:srcCompat="@drawable/farmer" />

            <TextView
                android:id="@+id/tv_dekin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_dekin"
                android:paddingLeft="9dp"
                android:paddingTop="9dp"
                android:paddingRight="9dp"
                android:text="Dekin"
                android:textAlignment="center"
                android:textColor="@color/accent"
                android:textSize="18sp" />

        </RelativeLayout>


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view3"
        android:layout_width="152dp"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:elevation="3dp"
        android:foregroundGravity="center"
        app:cardCornerRadius="9dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card_view">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_gede"
                android:layout_width="match_parent"
                android:layout_height="152dp"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:clickable="true"
                android:scaleType="centerCrop"
                android:src="@drawable/farmer"
                tools:srcCompat="@drawable/farmer" />

            <TextView
                android:id="@+id/tv_gede"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_gede"
                android:paddingLeft="9dp"
                android:paddingTop="9dp"
                android:paddingRight="9dp"
                android:text="Gedhe"
                android:textAlignment="center"
                android:textColor="@color/accent"
                android:textSize="18sp" />

        </RelativeLayout>


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_view4"
        android:layout_width="152dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:elevation="3dp"
        android:foregroundGravity="center"
        app:cardCornerRadius="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card_view2">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_gimpil"
                android:layout_width="match_parent"
                android:layout_height="152dp"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:clickable="true"
                android:scaleType="centerCrop"
                android:src="@drawable/farmer"
                tools:srcCompat="@drawable/farmer" />

            <TextView
                android:id="@+id/tv_gimpil"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_gimpil"
                android:paddingLeft="9dp"
                android:paddingTop="9dp"
                android:paddingRight="9dp"
                android:text="Hendra"
                android:textAlignment="center"
                android:textColor="@color/accent"
                android:textSize="18sp" />

        </RelativeLayout>


    </android.support.v7.widget.CardView>


        <!--</LinearLayout>-->
    <!--</android.support.v4.widget.NestedScrollView>-->



</android.support.constraint.ConstraintLayout>